<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hmelikyan.exceptionhandlerdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <queries>
        <package android:name="com.hmelikyan.exceptionhandler" />
    </queries>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExceptionHandler">

        <activity android:name="com.hmelikyan.exceptionhandlerdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="co.uk.MenuKey"
            android:value="test" />
    </application>

</manifest>